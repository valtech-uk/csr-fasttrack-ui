    <fieldset>
      <legend class="form-label">Do you want to submit these pass marks?</legend>
      <div class="form-group form-group-compound inline">
        <label data-target="passmarkset-panel" for="passmarkset-yes" class="block-label">
          <input name="passmarkset" type="radio" id="passmarkset-yes" aria-controls="passmarkset-panel" aria-expanded="false">Yes
        </label>
        <label for="passmarkset-no" class="block-label">
          <input name="passmarkset" type="radio" id="passmarkset-no">No
        </label>
      </div>
      <div id="passmarkset-panel" class="toggle-content" aria-hidden="true">
        <div class="panel-warning">
          <p class="text">Once submitted you won't be able to decrease the fail marks or increase the pass marks.</p>
        </div>
        <a href="" class="button" id="submitPassMarks">Submit pass marks</a>
        <p id="submittedTime"></p>
      </div>
    </fieldset>

<script>
  $(function(){
    $('#submitPassMarks').on('click', function(e) {
      var date = new Date(),
          time = date.getHours() + ":" + (date.getMinutes()<10?'0':'') + date.getMinutes(),
          day = date.getDate() + '/' + (date.getMonth() +1) + '/' + date.getFullYear();


      e.preventDefault();

      $('#passMarkSubmitted').show();

      $('#submittedTime').text('Last submitted: ' + time + ' on ' + day);
    });
  });
</script>